<div class="container">
  <div class="welcome">
    <div class="pinkbox">
      <div class="signup nodisplay">
        <h1>新規登録</h1>

        <form autocomplete="off">
          <input type="text" placeholder="username">
          <input type="email" placeholder="email">
          <input type="password" placeholder="password">
          <input type="password" placeholder="confirm password">
          <button class="button submit">アカウントを作成</button>
        </form>


      </div>



      <div class="signin">
        <h1>ログイン</h1>

        <form class="more-padding" autocomplete="off">
          <input type="text" placeholder="username">
          <input type="password" placeholder="password">
          <div class="checkbox">
            <input type="checkbox" id="remember" /><label for="remember">remember me</label>
          </div>
          <button class="button submit">ログイン</button>
        </form>



      </div>
    </div>





    <div class="leftbox">
      <h2 class="title">Melody<br>Forest</h2>
      <%# <p class="desc">pick your perfect</p> %>
      <img class="flower smaller" src="https://image.ibb.co/d5X6pn/1357d638624297b.jpg" alt="1357d638624297b" border="0">
      <p class="account">既にアカウントをお持ちの場合はこちら</p>
      <button class="button" id="signin">ログイン</button>
    </div>


    <div class="rightbox">
      <h2 class="title">Melody<br>Forest</h2>
      <%# <p class="desc"> pick your perfect </p> %>
      <img class="flower" src="https://preview.ibb.co/jvu2Un/0057c1c1bab51a0.jpg"/>
      <p class="account">アカウントをお持ちでない場合はこちら</p>
      <button class="button" id="signup">新規登録</button>
    </div>


  </div>
 </div>